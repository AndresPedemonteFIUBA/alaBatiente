FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "496";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0.78 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            slip;
    }
    flapTop
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    flapBottom
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    flapSide
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    cylinder
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


