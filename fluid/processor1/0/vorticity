FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      vorticity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    top
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    bottom
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    flapBottom
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    flapSide
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    flapTop
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    cylinder
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


